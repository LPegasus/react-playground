{
  "compilerOptions": {
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports":true,
    "target": "es6",
    "module": "es6",
    "allowJs": true,
    "declaration": false,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "diagnostics": true,
    "importHelpers": true
  },
  "exclude": [
    "node_modules"
  ]
}
